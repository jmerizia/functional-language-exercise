print_string str =>
    (if str
        (both
            (print_int (first str))
            (print_string (second str))
        )
        nil
    )

range a b =>
    (if (equals a b)
        nil
        (pair
            a
            (range
                (add a 1)
                b
            )
        )
    )

(print_string (pair 'a' nil))

