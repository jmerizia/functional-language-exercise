fib n =>
    (if (equals n 0)
        (0)
        (if (equals n 1)
            (1)
            (if (equals n 2)
                (1)
                (add
                    (fib (sub n 1))
                    (fib (sub n 2))
                )
            )
        )
    )

(print_int (fib 3))

